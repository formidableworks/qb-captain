{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "MaindataResponse.json",
  "title": "MaindataResponse",
  "description": "Qbittorrent webapi /api/v2/sync/maindata response.",
  "type": "object",
  "properties": {
    "rid": {
      "type": "number",
      "description": "Response ID"
    },
    "full_update": {
      "type": "boolean",
      "description": "Whether the response contains all the data or partial data"
    },
    "server_state": { "$ref": "TransferInfoResponse.json" },
    "torrents": {
      "type": "object",
      "description": "Dictionary of torrent-hash against torrent-attributes",
      "additionalProperties": { "$ref": "TorrentInfo.json" }
    }
  },
  "required": ["rid", "full_update"]
}

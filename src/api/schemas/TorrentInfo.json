{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "TorrentInfo.json",
  "title": "TorrentInfo",
  "description": "Qbittorrent webapi Torrent Info",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Torrent name"
    },
    "progress": {
      "type": "number",
      "description": "Torrent progress (percentage/100)"
    }
  },
  "required": ["name", "progress"]
}
